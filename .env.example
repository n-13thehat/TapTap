# Database (local docker default; Supabase optional)
DATABASE_URL=postgresql://postgres:password@localhost:5432/taptap_dev
SUPABASE_DB_URL=postgresql://postgres:password@localhost:5432/taptap_dev
DIRECT_URL=postgresql://postgres:password@localhost:5432/taptap_dev?sslmode=disable

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-nextauth-secret

# External API Keys
OPENAI_API_KEY=your-openai-api-key
YOUTUBE_API_KEY=your-youtube-api-key
DEEZER_APP_ID=your-deezer-app-id
DEEZER_APP_SECRET=your-deezer-app-secret
DEEZER_REDIRECT_URI=http://localhost:3000/api/deezer/callback
DEEZER_MARKET_DISABLED=false

# Application Configuration
TREASURE_KEY=your-treasure-key
TREASURY_PRIVATE_KEY=your-treasury-private-key
TAPTAP_ADMIN_EMAIL=admin@example.com
TAPTAP_APP_PASSWORD=your-app-password
NEXT_PUBLIC_ADMIN_PASSCODE=your-admin-passcode

# Optional OAuth Providers
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
APPLE_ID=your-apple-id
APPLE_SECRET=your-apple-secret

# Monitoring & Analytics
SENTRY_DSN=your-sentry-dsn
SENTRY_RELEASE=your-release-version
SENTRY_ENV=development

# Feature Flags
BETA_MODE=false
BETA_ACCESS_CODE=your-beta-code

# Development Settings
NODE_ENV=development
